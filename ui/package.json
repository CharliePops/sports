    "react-dom": "16.11.0"
  },
  "devDependencies": {
    "@types/node": "^12.11.6",
    "@types/react": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "eslint": "^6.6.0",
